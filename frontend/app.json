{
  "expo": {
    "name": "RoadCheck",
    "slug": "roadcheck",
    "version": "1.0.0",
    "orientation": "landscape",
    "scheme": "roadcheck",
    "icon": "./assets/background/login page.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "bundler": "metro"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      },
      "package": "com.ro7ello.roadcheck"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "newArchEnabled": true
    },
    "extra": {
      
      "router": {},
      "eas": {
        "projectId": "a8936058-bc96-47d8-a54c-2d7f568426ca"
      }
    },
    "ios": {
      "bundleIdentifier": "com.ro7ello.roadcheck",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    }
  }
}
