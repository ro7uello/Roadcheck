{
  "expo": {
    "name": "RoadCheck",
    "slug": "roadcheck",
    "version": "1.0.0",
    "orientation": "landscape",
    "scheme": "roadcheck",
    "userInterfaceStyle": "light",
    "API_URL": "http://localhost:5000",
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "bundler": "metro"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "newArchEnabled": true
    },
    "fonts": [
      "./assets/fonts/pixel3.ttf"
    ]
  }
}